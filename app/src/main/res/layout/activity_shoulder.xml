<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundwithoutlogo"
    tools:context=".ShoulderActivity">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="409dp"
        android:layout_height="332dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="382dp"
                android:layout_height="75dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"
                app:cardBackgroundColor="#2d3436"
                app:cardCornerRadius="20dp"
                app:cardElevation="8dp">

                <View
                    android:id="@+id/dissolveViewq"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/dissolve_gradient" />

                <Button
                    android:id="@+id/militarypressButton"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@string/app_name"
                    android:drawableLeft="@drawable/militarypress"
                    android:text="Military Press"
                    android:textColor="#FFFFFF"
                    android:textColorHighlight="#FFFFFF"
                    android:textColorHint="#FFFFFF"
                    android:textColorLink="#FFFFFF" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="382dp"
                android:layout_height="75dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                app:cardBackgroundColor="#2d3436"
                app:cardCornerRadius="20dp"
                app:cardElevation="8dp">

                <View
                    android:id="@+id/dissolveVieww"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/dissolve_gradient" />

                <Button
                    android:id="@+id/seatedDumbellPressButton"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@string/app_name"
                    android:drawableLeft="@drawable/seateddumbellpress"
                    android:text="Seated Dumbell Press"
                    android:textColor="#FFFFFF"
                    android:textColorHighlight="#FFFFFF"
                    android:textColorHint="#FFFFFF"
                    android:textColorLink="#FFFFFF" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="382dp"
                android:layout_height="75dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                app:cardBackgroundColor="#2d3436"
                app:cardCornerRadius="20dp"
                app:cardElevation="8dp">

                <View
                    android:id="@+id/dissolveViewe"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/dissolve_gradient" />

                <Button
                    android:id="@+id/cablesideButton"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@string/app_name"
                    android:drawableLeft="@drawable/lateral"
                    android:text="Cable Side Lateral Raises"
                    android:textColor="#FFFFFF"
                    android:textColorHighlight="#FFFFFF"
                    android:textColorHint="#FFFFFF"
                    android:textColorLink="#FFFFFF" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="382dp"
                android:layout_height="75dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                app:cardBackgroundColor="#2d3436"
                app:cardCornerRadius="20dp"
                app:cardElevation="8dp">

                <View
                    android:id="@+id/dissolveViewr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/dissolve_gradient" />

                <Button
                    android:id="@+id/benchDipButton"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@string/app_name"
                    android:drawableLeft="@drawable/dip"
                    android:text="Bench Dip"
                    android:textColor="#FFFFFF"
                    android:textColorHighlight="#FFFFFF"
                    android:textColorHint="#FFFFFF"
                    android:textColorLink="#FFFFFF" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="382dp"
                android:layout_height="75dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                app:cardBackgroundColor="#2d3436"
                app:cardCornerRadius="20dp"
                app:cardElevation="8dp">

                <View
                    android:id="@+id/dissolveViewt"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/dissolve_gradient" />

                <Button
                    android:id="@+id/peckdeckFliesButton"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@string/app_name"
                    android:text="Not Available"
                    android:textColor="#FFFFFF"
                    android:textColorHighlight="#FFFFFF"
                    android:textColorHint="#FFFFFF"
                    android:textColorLink="#FFFFFF" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/fillingView"
                android:layout_width="382dp"
                android:layout_height="60dp"
                android:layout_weight="1" />
        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/exercisesTxt9"
        android:layout_width="233dp"
        android:layout_height="38dp"
        android:fontFamily="serif-monospace"
        android:text="Exercises"
        android:textColor="#e6e6e6"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.63"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.064" />

    <TextView
        android:id="@+id/workoutTxt2"
        android:layout_width="150dp"
        android:layout_height="38dp"
        android:fontFamily="serif-monospace"
        android:text="Workouts"
        android:textAllCaps="false"
        android:textColor="#e6e6e6"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cardView"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.061" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#2d3436"
        app:cardCornerRadius="16dp"
        app:cardElevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.22000003">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="382dp"
            android:layout_height="320dp"
            android:backgroundTint="#05ae1937"
            android:contentDescription="@string/app_name"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/shoulder"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="110dp" />

    </androidx.cardview.widget.CardView>

    <Button
        android:id="@+id/backbutton5"
        android:layout_width="48dp"
        android:layout_height="52dp"
        android:layout_weight="1"
        android:backgroundTint="#10361938"
        android:contentDescription="@string/app_name"
        android:drawableTop="@drawable/backbuttoninapp"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/exercisesTxt9"
        app:layout_constraintHorizontal_bias="0.360"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0590"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/longbackground4"
        android:layout_width="336dp"
        android:layout_height="50dp"
        android:layout_weight="1"
        android:backgroundTint="#70391a3a"
        android:clickable="false"
        android:contentDescription="@string/app_name"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:cornerRadius="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cardView"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/cardView"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.970"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/smallMembershipButton5"
        android:layout_width="72dp"
        android:layout_height="52dp"
        android:layout_weight="1"
        android:backgroundTint="#10361938"
        android:contentDescription="@string/app_name"
        android:drawableTop="@drawable/smallmembership"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/longbackground4"
        app:layout_constraintEnd_toEndOf="@+id/longbackground4"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="@+id/longbackground4"
        app:layout_constraintTop_toTopOf="@+id/longbackground4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/smallExerciseButton5"
        android:layout_width="73dp"
        android:layout_height="52dp"
        android:layout_weight="1"
        android:backgroundTint="#10361938"
        android:contentDescription="@string/app_name"
        android:drawableTop="@drawable/smalldumbell"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/longbackground4"
        app:layout_constraintEnd_toEndOf="@+id/longbackground4"
        app:layout_constraintHorizontal_bias="0.343"
        app:layout_constraintStart_toStartOf="@+id/longbackground4"
        app:layout_constraintTop_toTopOf="@+id/longbackground4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/smallScheduleButton5"
        android:layout_width="73dp"
        android:layout_height="52dp"
        android:layout_weight="1"
        android:backgroundTint="#10361938"
        android:contentDescription="@string/app_name"
        android:drawableTop="@drawable/smallschedule"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/longbackground4"
        app:layout_constraintEnd_toEndOf="@+id/longbackground4"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toStartOf="@+id/longbackground4"
        app:layout_constraintTop_toTopOf="@+id/longbackground4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/smallSupplementsButton5"
        android:layout_width="73dp"
        android:layout_height="52dp"
        android:layout_weight="1"
        android:backgroundTint="#10361938"
        android:contentDescription="@string/app_name"
        android:drawableTop="@drawable/smallsupplements"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/longbackground4"
        app:layout_constraintEnd_toEndOf="@+id/longbackground4"
        app:layout_constraintHorizontal_bias="0.923"
        app:layout_constraintStart_toStartOf="@+id/longbackground4"
        app:layout_constraintTop_toTopOf="@+id/longbackground4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>